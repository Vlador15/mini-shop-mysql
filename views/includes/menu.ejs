<div class="menu-page__header">
   <div class="container-fluid">
      <div class="box-search">
         <form action="/search" method="post" enctype="multipart/form-data" id="search" class="header-search relative">
            <input type="text" name="search" value="" placeholder="Найти товары" class="input-lg">
            <button type="submit" class="search-btn">
               <svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="icon">
                  <circle cx="11" cy="11" r="8"></circle>
                  <line x1="21" y1="21" x2="16.65" y2="16.65"></line>
               </svg>
            </button>
         </form>

         <div class="cart-order" style="margin-right: 35px;">
            <a href="/orders" class="cart-link relative">Мои заказы</a>
         </div>
         <div class="header-cart cart-profile">
            <a href="/cart" class="cart-link relative">
               <svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="icon">
                  <circle cx="9" cy="21" r="1"></circle>
                  <circle cx="20" cy="21" r="1"></circle>
                  <path d="M1 1h4l2.68 13.39a2 2 0 0 0 2 1.61h9.72a2 2 0 0 0 2-1.61L23 6H6"></path>
               </svg>
               <span class="user-nav__title mobile-menu__title">Корзина</span>
            </a>
         </div>
      </div>
   </div>
</div>

<nav id="menu" class="navbar">
   <div class="collapse navbar-collapse navbar-ex1-collapse">
      <ul class="nav navbar-nav" style="width: 100%">
         <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" style="background-color: #f99000;"> 
               Каталог
            </a> 

            <div class="dropdown-menu" >
               <div class="dropdown-inner">
                  <ul class="list-unstyled one">   
                     <% session.menu.forEach(item => { %>
                        <li><a href="/catalog/<%- item.path %>"><%- item.name %></a></li> 
                     <% }) %> 
                  </ul> 
               </div>  
            </div>
         </li> 
         <li><a href="/">Главная</a></li> 
         <li><a href="/about">О магазине </a></li>
         <li><a href="/contacts">Контакты</a></li>

         <% if ( session.auth != false && session.auth != undefined ) { %> 

         <li id="menu-link-user" style="float: right;"><a href="/logout">Выйти <i style="color: red;" class="fa fa-sign-out" aria-hidden="true"></i></a></li>
         <li id="menu-link-user" style="float: right;"><a href="/profile">Кабинет</a></li> 

         <% if ( session.isAdmin > 0 ) { %>
            <li id="menu-link-user" class="dropdown" style="float: right;">
               <a href="#" class="dropdown-toggle" data-toggle="dropdown" style="background-color: #8f8f90"> 
                 Админ
               </a> 
                

               <div class="dropdown-menu">
                  <div class="dropdown-inner">
                     <ul class="list-unstyled one"> 
                        <li><a href="/admin/orders">Все заказы</a></li>
                        <li><a href="/admin/create-product">Добавить продукт</a></li>
                        <li><a href="/admin/create-category">Добавить категорию</a></li> 
                        <li><a href="/admin/category">Список категорий</a></li> 
                     </ul> 
                  </div> 
               </div>
            </li>  
         <% } %>    
        
         <% } else { %> 
         
            <li id="menu-link-user" style="float: right;">
               <a href="/login" style="background-color: #616169">
                  <svg viewBox="0 0 24 24" width="18" height="18" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="icon">
                     <path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path>
                     <circle cx="12" cy="7" r="4"></circle>
                  </svg>
                  Войти
               </a>
            </li> 
         <% } %> 
      </ul>
   </div>
</nav>
<nav id="start"></nav>